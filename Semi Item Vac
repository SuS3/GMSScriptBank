[ENABLE]
alloc(Hook,64)

Hook:
call 004E7E20// Original Call
mov ecx,eax
mov eax,[esp+0C]
//mov edi,[0225E764]// CUserLocal: 8B 3D ? ? ? ? 8B 40
//mov edi,[edi+12384] // Character X Location Offset: 89 8E ? ? ? ? 8B 50 ? 8B 06 89 96 ? ? ? ? 8B 50
//add edi,#0 // X
mov edi, #100 // X - comment this line and uncomment above for drop at char location
mov [eax],edi
pop edi
//mov ecx,[0225E764]
//mov ecx,[ecx+12384+04]
//add ecx,#0 // Y
mov ecx, #381 // Y 0 comment this line and uncomment above for drop at char location
mov [eax+04],ecx
pop esi
ret 0004


005DBCD4:
jmp Hook


[DISABLE]
005DBCD4: // E8 ? ? ? ? 8B C8 8B 44 24 ? 89 38 5F 89 48 ? 5E C2 04 00 CC CC CC CC CC CC CC 56 [2nd result]
call 004E7E20

dealloc(Hook)
