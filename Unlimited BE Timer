[enable]
alloc(SkillTimer,1024)
label(AppearUnlimited)

00993DDA://89 86 ? ? ? ? 89 9e ? ? ? ? 89 9e ? ? ? ? e8 ? ? ? ? 83 ? ? 85 c0
jmp SkillTimer
db 90
AppearUnlimited:

SkillTimer:
mov [esi+000000F8],7FFFFFFF
jmp AppearUnlimited


[disable]
00993DDA:
mov [esi+000000F8],eax
