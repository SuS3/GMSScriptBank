//175.1
//Updated by Randomcity2
[ENABLE]
alloc(BlockRecvHook,256)
label(RecvCheckLoop)
label(BlockRecv)
label(End)

alloc(RecvList,512)

//Hook address: 008078D0
//8B ? 24 ? 3D ? ? ? ? 0F 8F ? ? ? ? 0F 84 ? ? ? ? 8D

BlockRecvHook:
mov eax,[esp+04] //Original Opcode @ Hook address
and eax,0FFFF
push edx
mov edx,RecvList

RecvCheckLoop:
cmp eax,[edx]
je BlockRecv
cmp [edx],00
je End
add edx,04
jmp RecvCheckLoop

BlockRecv:
xor eax,eax

End:
pop edx
cmp eax,00000422 //Opcode below Hook address
jmp 0083D7C9 //Address of jg

RecvList:
dd 01E6 //Block Pierre's Hat/Vellum Rocks
// dd 03BF // Block Kanna's Barrier animation
// dd B900 //or // dd 00B9 // to allow vac items
// dd 4100 //or // dd 0041 // block pet loot lag
dd 00 //This signifies the end of the list DO NOT REMOVE; add headers above it

01EAAC98: //4 Bytes scan Hook address [1st Result]
dd BlockRecvHook

[DISABLE]
01EAAC98: //Same as above
dd 0083D7C0 //Hook address

dealloc(BlockRecv)
dealloc(RecvList)
